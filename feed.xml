                    history.go(-1);
                    if (pass1 == &quot;7654321&quot;) 
                    {
                        alert('right!');
                        break;
                    }
                    testV+=1;
                    var pass1 =
                    prompt('wrong:');
                }
                if (pass1!=&quot;password&quot; &amp; testV ==3)
                history.go(-1);
                return &quot; &quot;;
            }
            document.write(password());
